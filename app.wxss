/**app.wxss**/

view, image, text, navigator, .container {
  box-sizing: border-box;
  padding: 0;
  margin: 0;
}

/* flower-template */

.flower-template {
  font-size: 28rpx;
  color: #fab9ab;
  position: relative;
}

.flower-template::after {
  content: '';
  display: block;
  position: absolute;
  background-image: url('data:image/png;base64,');
  width: 23rpx;
  height: 23rpx;
  right: -34rpx;
  background-size: cover;
  bottom: 8rpx;
}
